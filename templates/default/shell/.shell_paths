export SVN_EDITOR="vim"
TEMPORARY_EXEC_PATHS="$HOME/src/nanoc-fork/nanoc/bin:$HOME/src/ebay_template/bin:$HOME/src/nanoc/bin"
APP_ENGINE_PATH="/usr/local/google_appengine"
APPENGINE_JAVA_SDK="$HOME/src/appengine-java-sdk"
APPENGINE_JAVA_SDK_PATH="$HOME/src/appengine-java-sdk/bin"
TEN_GEN_PATH="/usr/local/10gen"
export GAEO_HOME='/Users/nat/opt/local/gaeo'
JRUBY_EDGE_PATH="$HOME/src/jruby/bin"
USER_PATHS="$HOME/opt/bin:$GAEO_HOME/bin:$HOME/.gems/bin:$HOME/bin"

export PATH="$USER_PATHS:$LIFT_PATH:$TEMPORARY_EXEC_PATHS:$APP_ENGINE_PATH:$APPENGINE_JAVA_SDK_PATH:/opt/local/bin:/usr/local/bin:/bin:/usr/bin:/opt/local/lib/postgresql83/bin:/usr/X11R6/bin:/usr/local/lib/jruby/bin:/Applications/Shoes.app/Contents/MacOS:/sbin:/usr/sbin"

export MANPATH="/local/man:/opt/local/share/man:$MANPATH"
export INFOPATH="/local/info:/opt/local/share/info:$INFOPATH"
export LC_CTYPE="en_US.UTF-8"

export GEM_HOME="$HOME/.gems"

# Textmate
export TM_SUPPORT_PATH="/Users/nat/Library/Application Support/TextMate/Support"

# compiling
export CFLAGS="-I/local/include -I/opt/local/include -L/local/lib -L/opt/local/lib"
export CPPFLAGS="$CFLAGS"
export LDFLAGS="-L/local/lib -L/opt/local/lib"

export PYTHONSTARTUP=~/.pystartup
export UNISONLOCALHOSTNAME=brite

# pkg-config and autotools
export PKG_CONFIG_PATH="/usr/lib/pkgconfig:/usr/X11R6/lib/pkgconfig:/local/lib/pkgconfig:/opt/local/lib/pkgconfig"
export ACLOCAL_FLAGS="-I /local/share/aclocal -I /opt/local/share/aclocal -I /usr/share/aclocal"

# custom export shit
export SCVIM_DIR=/usr/local/share/scvim
